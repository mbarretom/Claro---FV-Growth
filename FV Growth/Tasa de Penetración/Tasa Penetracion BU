            WHEN A.P_CONVERGENCIA = 'TRUE' AND A.B_HOGAR = 'TRUE' AND CANAL_NOTIFICATION_PUSH_APP= 'TRUE' AND C.BUSINESS_UNIT = 'CONVERGENCIA'
